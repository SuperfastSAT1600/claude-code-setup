{
  "name": "blog-agent-sat",
  "version": "1.0.0",
  "type": "module",
  "description": "AI-powered blog writing agent for SAT preparation content",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "blog:generate": "tsx blog-agent/src/index.ts",
    "blog:dev": "tsx watch blog-agent/src/index.ts",
    "blog:build": "tsc -p blog-agent/tsconfig.json",
    "blog:start": "node blog-agent/dist/index.js",
    "blog:migrate": "tsx blog-agent/scripts/migrate-posts-to-supabase.ts",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "check": "npm run format:check && npm run lint && npm run typecheck",
    "fix": "npm run format && npm run lint:fix",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "audit": "npm audit",
    "deps:check": "depcheck",
    "deps:unused": "ts-prune",
    "analyze": "echo \"Add bundle analyzer command here\""
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.11.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.2.0",
    "autoprefixer": "^10.4.24",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "license-checker": "^25.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.2.0",
    "tailwindcss": "^3.4.19",
    "ts-prune": "^0.10.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@hookform/resolvers": "^5.2.2",
    "@supabase/supabase-js": "^2.95.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.1",
    "enquirer": "^2.4.1",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.563.0",
    "marked": "^17.0.1",
    "next": "^14.2.35",
    "openai": "^6.19.0",
    "pdf-parse": "^2.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
